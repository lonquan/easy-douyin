{
  "name": "antcool/easy-douyin",
  "description": "A easy-to-use PHP SDK for Douyin Open Platform",
  "version": "0.0.1",
  "type": "library",
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "ext-openssl": "*",
    "nyholm/psr7-server": "^1.1",
    "nyholm/psr7": "^1.8",
    "illuminate/container": "^10.48",
    "illuminate/collections": "^10.48",
    "psr/simple-cache": "^3.0",
    "symfony/cache": "^6.4",
    "symfony/http-client": "^6.4",
    "monolog/monolog": "^3.7"
  },
  "license": "MIT",
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "EasyDouyin\\": "src/"
    }
  },
  "authors": [
    {
      "name": "龙权",
      "email": "root@antcool.com"
    }
  ],
  "require-dev": {
    "symfony/var-dumper": "^6.4",
    "nesbot/carbon": "^3.7",
    "symfony/http-foundation": "^6.4",
    "symfony/http-kernel": "^6.4"
  }
}
